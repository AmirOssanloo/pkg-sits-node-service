{
  "name": "@amirossanloo/node-service",
  "version": "0.0.0",
  "author": "Amir Ossanloo <amir@amirossanloo.com>",
  "private": true,
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git://github.com/amirossanloo/pkg-sits-node-service.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "format": "prettier --write",
    "lint": "turbo run lint --parallel && prettier --check",
    "lint:fix": "turbo run lint:fix --parallel && prettier --write",
    "test": "turbo run test --concurrency=1",
    "type:check": "turbo run type:check --parallel",
    "release": "tsx bin/publish.sh"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.2",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/eslint": "^9.6.1",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.9",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.0.15",
    "@types/node-fetch": "^2.6.12",
    "@types/ramda": "^0.30.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-config-standard": "^17.1.0",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-webpack": "^0.13.10",
    "eslint-plugin": "^1.0.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-security": "^3.0.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "lint-staged": "^16.1.2",
    "node-fetch": "2.6.7",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "turbo": "^2.3.4",
    "typescript": "^5.8.3"
  },
  "jest-junit": {
    "outputName": "test-report.xml"
  },
  "engines": {
    "node": ">=22.16.0"
  },
  "volta": {
    "node": "22.16.0"
  },
  "packageManager": "pnpm@10.10.0",
  "dependencies": {
    "dotenv": "^17.2.1"
  }
}
